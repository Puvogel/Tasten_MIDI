// Main File
#include "ButtonsAndLEDs.h"
#include "MIDIHandler.h"

void setup() {
  Serial.begin(115200);
  midiSerial.begin(31250);

  setupPins();
  loadProfile(currentProfile);
  updateProfileLEDs();
}

void loop() {
  handleButtons();
  if (calibrationMode) {
    handleCalibrationBlink();
  } else {
    handleSensors();
  }
}
